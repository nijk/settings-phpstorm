<application>
  <component name="FindSettings">
    <option name="regularExpressions" value="true" />
    <option name="REGULAR_EXPRESSIONS" value="true" />
    <recentFileMasks>
      <mask>*.properties</mask>
      <mask>*.html</mask>
      <mask>*.jsp</mask>
      <mask>*.xml</mask>
      <mask>*.java</mask>
      <mask>*.as</mask>
      <mask>*.css</mask>
      <mask>*.mxml</mask>
      <mask>*.js</mask>
      <mask>*.scss</mask>
      <mask>*.php</mask>
      <mask>*.module</mask>
      <mask>*.tpl.php</mask>
      <mask>!*.tpl.php</mask>
      <mask>!*.tpl.php|!*.js</mask>
      <mask>!*.tpl.php&amp;&amp;!*.js</mask>
      <mask>!*.tpl.php,!*.js</mask>
      <mask>*.inc</mask>
      <mask>*.info</mask>
      <mask>*.php,*.module*.inc</mask>
      <mask>*.module,*.inc</mask>
    </recentFileMasks>
  </component>
</application>